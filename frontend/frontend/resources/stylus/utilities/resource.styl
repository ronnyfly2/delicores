body
	user-select none
	padding 0px
	font-size $baseFontPx
	padding-top 148px
	position relative
	background-color #f5f5f7
	&.fixmodal
		padding-top 90px
		header
			.menu-header
				display none
	&.fixsearch
		padding-top 0
		header
			position relative
			.menu-header
				display none
		footer
			margin-top 0
.container
	font-size 0
.row
	*zoom 1
	&:before,&:after
		display table
		content ""
		line-height 0
	&:after
		clear both
.loader_lock
	width auto
	body
		overflow hidden !important
.box_center
	max-width $wrapperWidth
	margin 0 auto
	position relative
	padding 0 15px
	*zoom 1
	&:before,&:after
		display table
		content ""
		line-height 0
	&:after
		clear both
.loader
	background rgba(250,250,250,0.5)
	height 100%
	left 0
	position absolute
	top 0
	width 100%
	z-index 4
	span
		background url($imgPath +"loader-page.gif")
		height 35px
		left 50%
		margin -16px 0 0 -16px
		position absolute
		top 50%
		width 35px

.clear
	clear both

// Position
.relative
	position relative
.absolute
	position absolute
.fixed
	position fixed
// Float
.right
	float right
.left
	float left
// Aligns
.text_center
	text-align center
.text_left
	text-align left
.text_right
	text-align right
// Text
.upper
	text-transform uppercase
.lower
	text-transform lowercase
.hack_space
	height 100%
	width 0px
	display inline-block
	vertical-align middle
.btn
	background-color $greyPrincipal
	border 0
	border-radius 4px
	color white
	cursor pointer
	display inline-block
	font-family $EncdSExSemiBold
	font-size 0.875rem
	line-height 2.4
	margin 0
	min-width 125px
	padding 0 20px
	outline none
	text-align center
	&_color_primary
		background-color $PrimaryColor
	&_medium
		font-size 0.8rem
	&_large
		font-size 1rem
		line-height 3
.is_round
	border-radius 20px




